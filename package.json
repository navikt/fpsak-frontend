{
  "name": "fpsak-frontend",
  "description": "Klient for FPSAK",
  "main": "./dist/index.js",
  "author": "NAV",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/fpsak-frontend.git"
  },
  "scripts": {
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu mochapack --colors --webpack-config webpack/webpack.test.js \"packages/**/*.spec.*\"",
    "test:watch": "cross-env NODE_ICU_DATA=./node_modules/full-icu mochapack --watch --webpack-config webpack/webpack.test.watch.js \"packages/**/*.spec.*\"",
    "test:coverage": "cross-env NODE_ICU_DATA=./node_modules/full-icu nyc mochapack --webpack-config webpack/webpack.test.coverage.js \"packages/**/*.spec.*\"",
    "dev": "cross-env NODE_ENV=development NODE_ICU_DATA=./node_modules/full-icu node webpack/webpack.dev.server.js --config webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --no-progress --colors --config webpack/webpack.prod.js",
    "lerna": "lerna",
    "lerna:publish": "(lerna updated && lerna publish --cd-version patch --yes -m \"publish [ci skip]\") || echo \"Ingen endringer avslutter OK\"",
    "less:lint": "stylelint \"packages/**/*.less\"",
    "ts-check": "tsc --pretty",
    "doc": "react-asciidoc-generator ./packages/ -o ../../docs/generated/react.adoc --handlebar-template src/docs/asciidoc-template.handlebars",
    "up": "docker-compose up -d --remove-orphans --build",
    "down": "docker-compose down",
    "lint": "./node_modules/.bin/eslint ./packages/**/*.jsx",
    "remove-node-modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "lint:config": "node -e 'console.dir(require(\"./.eslintrc.js\"))'",
    "storybook": "start-storybook -p 9001 -c packages/storybook/.storybook",
    "deploy-storybook": "build-storybook -c packages/storybook/.storybook -o .static_storybook && storybook-to-ghpages --existing-output-dir=.static_storybook"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@babel/runtime": "^7.9.6",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "dotenv": "^8.0.0",
    "history": "^4.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "3.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@commercetools/enzyme-extensions": "^5.0.0",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-centered": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/cli": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.5",
    "@storybook/theming": "^5.3.18",
    "@svgr/webpack":  "^5.4.0",
    "@types/chai": "^4.2.11",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.5",
    "@types/mocha": "^7.0.2",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.35",
    "@types/react-collapse": "^5.0.0",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-form": "^8.2.5",
    "@types/sinon": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "axios-mock-adapter": "^1.18.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "body-parser": "*",
    "cache-loader": "^4.1.0",
    "chai": "^4.2.0",
    "circular-dependency-plugin": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^1.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-lerna": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "full-icu": "^1.3.1",
    "html-webpack-plugin": "^4.3.0",
    "ignore-styles": "^5.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^16.2.2",
    "json-loader": "^0.5.7",
    "lerna": "^3.20.2",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.2",
    "mochapack": "^1.1.15",
    "null-loader": "^4.0.0",
    "nyc": "^15.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.3.3",
    "stylelint-order": "^4.0.0",
    "terser-webpack-plugin": "^3.0.1",
    "thread-loader": "^2.1.2",
    "typescript": "3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1"
  },
  "resolutions": {
    "react-test-renderer": "^16.13.1"
  },
  "nyc": {
    "branches": 40,
    "lines": 60,
    "functions": 50,
    "statements": 50,
    "cache": false,
    "check-coverage": true,
    "require": [
      "@babel/register",
      "./setup/setup.jsx"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "text",
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".jsx",
      ".tsx",
      ".ts",
      ".es6"
    ],
    "exclude": [
      "packages/**/*.spec.jsx",
      "packages/**/*.spec.tsx",
      "packages/**/*.spec.ts",
      "packages/storybook/**",
      "packages/utils-test/**"
    ]
  }
}
