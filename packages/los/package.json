  "scripts": {
    "los-test": "mochapack --webpack-config webpack/webpack.test.js \"src/client/app/**/*.spec.ts*\"",
    "los-test:watch": "mochapack --watch --webpack-config webpack/webpack.test.watch.js \"src/client/app/**/*.spec.ts*\"",
    "los-test:coverage": "nyc mochapack --webpack-config webpack/webpack.test.coverage.js  \"src/client/app/*/**/*.ts*\"",
    "los-dev": "cross-env NODE_ENV=development node webpack/webpack.dev.server.js --config webpack/webpack.dev.js",
    "los-build": "cross-env NODE_ENV=production webpack --progress --colors --config webpack/webpack.prod.js",
    "los-postinstall": "yarn run los-build && yarn los-test && yarn run los-less:lint",
    "los-ts-check": "tsc --pretty",
    "los-less:lint": "lesshint --max-warnings 0 --reporter ./_scripts/lesshint-reporter.js src/client/app -e *Source-Sans*.less",
    "los-doc": "react-asciidoc-generator ./src/client/app/ -o ../../docs/generated/react.adoc --handlebar-template src/docs/asciidoc-template.handlebars "
  },
  "nyc": {
    "require": [
      "@babel/register",
      "./src/client/testHelpers/setup.js"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
