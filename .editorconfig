# top-most EditorConfig file
root = true

# Windows-style newlines with a newline ending every file
[**]
charset = utf-8
end_of_line = lf
insert_final_newline = true

# css
[**.less]
indent_style = space
indent_size = 2

# javascript
[**.jsx]
indent_style = space
indent_size = 2
trim_trailing_whitespace=true
